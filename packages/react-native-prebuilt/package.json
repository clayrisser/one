{
  "name": "@vxrn/react-native-prebuilt",
  "type": "module",
  "version": "1.1.200",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "license": "MIT",
  "files": [
    "dist",
    "vendor",
    "src",
    "types"
  ],
  "scripts": {
    "prebuild": "node -r esbuild-register ./prebuild-react-native.ts",
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./react": "./vendor/react/index.js",
    "./jsx-runtime": "./vendor/react/jsx-dev-runtime.js"
  },
  "dependencies": {
    "@vxrn/vite-flow": "1.1.200",
    "@vxrn/vite-native-hmr": "1.1.200",
    "esbuild": "^0.23.0",
    "fs-extra": "^11.2.0",
    "import-meta-resolve": "^4.1.0",
    "metro-react-native-babel-preset": "^0.77.0"
  },
  "devDependencies": {
    "@tamagui/build": "^1.109.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.74.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
